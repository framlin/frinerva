{
  "name": "frinerva",
  "productName": "Frinerva",
  "version": "20220721",
  "description": "property management",
  "main": "app/main/main.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "jest --watchAll",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "watch": "nodemon --ignore 'app/common/persitence/data/*'  --exec electron-forge start"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "out", "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framlin/frinerva.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Wolfgang Egger",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@tsconfig/node16": "^1.0.3",
    "electron": "^19.0.8",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19"
  },
  "bugs": {
    "url": "https://github.com/electron/electron-quick-start/issues"
  },
  "homepage": "https://github.com/electron/electron-quick-start#readme",
  "dependencies": {
    "@types/node": "^18.8.0",
    "@types/uuid": "^8.3.4",
    "csv-reader": "^1.0.9",
    "electron-squirrel-startup": "^1.0.0",
    "inquirer-file-tree-selection-prompt": "^1.0.19",
    "jest-environment-jsdom": "^29.0.3",
    "jsonpath": "^1.1.1",
    "minimist": "^1.2.6",
    "uuid": "^9.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "frinerva"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "frinerva"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
